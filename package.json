    "dayjs": "^1.11.7",
    "dompurify": "3.2.6",
    "echarts": "^5.4.2",
    "express": "^4.21.2",
    "fabric": "^5.3.0",
    "html2canvas": "^1.4.1",
    "imports-loader": "^4.0.1",
    "jquery-mousewheel": "^3.1.13",
    "jquery-ui": "^1.13.2",
    "json-server": "^0.17.1",
    "jsts": "^2.9.3",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "ol": "^9.2.3",
    "ol-ext": "^4.0.11",
    "pinia": "^2.1.4",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
